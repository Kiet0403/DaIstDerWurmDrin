            <Cursor fx:constant="DISAPPEAR" />
         </cursor>
      </ImageView>
      <VBox fx:id="difficultyPane" layoutX="311.0" layoutY="456.0" spacing="25.0" visible="false">
         <children>
            <Button fx:id="easyButton" mnemonicParsing="false" onAction="#newGame" prefHeight="25.0" prefWidth="103.0" text="Easy" />
            <Button fx:id="mediumButton" mnemonicParsing="false" onAction="#newGame" prefHeight="25.0" prefWidth="103.0" text="Medium" />
            <Button fx:id="hardButton" mnemonicParsing="false" onAction="#newGame" prefHeight="25.0" prefWidth="103.0" text="Hard" />
            <Button mnemonicParsing="false" onAction="#back_to_Menu" prefHeight="25.0" prefWidth="103.0" text="Back" />
         </children>
      </VBox>
   </children>
</AnchorPane>
