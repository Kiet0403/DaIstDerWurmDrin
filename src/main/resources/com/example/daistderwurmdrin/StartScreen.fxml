<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<AnchorPane fx:id="startPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="723.0" prefWidth="726.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.daistderwurmdrin.HelloController">
   <children>
      <ImageView fx:id="wurmImage" fitHeight="723.0" fitWidth="740.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../icon.jpg" />
         </image>
      </ImageView>
      <VBox layoutX="303.0" layoutY="482.0" spacing="50.0">
         <children>
            <Button fx:id="playButton" mnemonicParsing="false" onAction="#chooseDifficulty" prefHeight="25.0" prefWidth="119.0" text="Play" />
            <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quit" prefHeight="25.0" prefWidth="119.0" text="Quit" />
         </children>
      </VBox>
      <Circle fill="#1ec8db" layoutX="65.0" layoutY="666.0" onMouseClicked="#Playmusic" radius="32.0" stroke="BLACK" strokeType="INSIDE" />
